script/source = "extends Button

@onready var control_2: Control = $\"../..\"
@onready var 服务器: Node = $\"../../服务器\"
func _ready() -> void:
	grab_focus()

func _pressed() -> void:
	match control_2.mosi:
		1:
			if OS.has_feature(\"android\"):
				if not OS.request_permission(\"android.permission.MANAGE_EXTERNAL_STORAGE\"):
					pass
				if not OS.request_permission(\"android.permission.READ_MEDIA_VISUAL_USER_SELECTED\"):
					pass
				if not OS.request_permission(\"android.permission.READ_MEDIA_AUDIO\"):
					pass
				if not OS.request_permission(\"android.permission.READ_MEDIA_IMAGES\"):
					pass
				if not OS.request_permission(\"android.permission.READ_MEDIA_VIDEO\"):
					pass
			$FileDialog.current_path=服务器.Fx
			$FileDialog.popup_centered()
		2:
			$FileDialog2.current_path=服务器.Up
			$FileDialog2.popup_centered()
func _on_file_dialog_file_selected(path: String) -> void:
	match control_2.mosi:
		1:
			服务器.Fx=path
		2:
			服务器.Up=path
	control_2.suaxing()
"

