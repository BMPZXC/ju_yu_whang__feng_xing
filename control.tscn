script/source = "extends AnimationPlayer
signal mosi
var jiesu=true
var b:int=0
@onready var 服务器: Node = $\"../../服务器\"

func _ready() -> void:
	b=int(服务器.Duqu(\"mosi\",\"a\"))
	if b==0:
		play_section(\"1\",0,0.1)
	else :
		play_section(\"1\",0.9,1)
	mosi.emit(b+1)
func qie():
	if not jiesu:return
	jiesu=false
	if b==0:
		play(\"1\")
	else :
		play_backwards(\"1\")
	b=(b+1)%2
	服务器.Sezi(\"mosi\",\"a\",str(b))
	var a=b+1
	mosi.emit(a)
	await animation_finished
	jiesu=true
	
"

