script/source = "extends Control
@onready var 服务器: Node = $服务器
@onready var 二维码: Node = $二维码
var mosi=0
var r:bool=false
@onready var texture_rect_2: TextureRect = $Control/Control/Control/TextureRect2
func _ready() -> void:
	_request_permissions()
	erweima(服务器.ip,texture_rect_2)
	erweima(\"%s%d\" % [服务器.ip, 服务器.version])
	r=true
	suaxing()
	

func _request_permissions():
	if OS.has_feature(\"android\"):
		# 确保权限已授予
		OS.request_permission(\"android.permission.WRITE_EXTERNAL_STORAGE\")
		OS.request_permission(\"android.permission.READ_EXTERNAL_STORAGE\")
		OS.request_permission(\"android.permission.INTERNET\")
			
		# Android 11+需要额外权限
		if Engine.get_version_info().major >= 4 and Engine.get_version_info().minor >= 2:
			OS.request_permission(\"android.permission.ACCESS_MEDIA_LOCATION\")

func _on_animation_player_mosi(a:int) -> void:
	mosi=a
	suaxing()

	
@onready var control: Control = $Control/Control/Control
@onready var button: Button = $Control/Button
@onready var texture_rect: TextureRect = $Control/Control/Control/TextureRect
@onready var label: Label = $Control/Control/Label

func suaxing():
	if !r:return
	match mosi:
		1:
			button.text=服务器.Fx.get_file()
			label.text=\"%s%d\" % [服务器.ip, 服务器.version]
			if button.text==\"\":
				button.text=\"选择文件\"
				control.visible=false
				return
			control.visible=true
			if texture_rect.visible:
				erweima(label.text)
			else :
				texture_rect.visible=true
		2:
			label.text=服务器.ip
			button.text=服务器.Up.get_file()
			if button.text==\"\":
				button.text=\"选择文件\"
				control.visible=false
				return
			control.visible=true
			texture_rect.visible=false

func erweima(a:String,texture:TextureRect=texture_rect):
	print(a)
	var image_data: PackedByteArray = 二维码.GenerateQRCodeBytes(a)
	var img = Image.new()
	if img.load_png_from_buffer(image_data) == OK:
		texture.texture = ImageTexture.create_from_image(img)
"

